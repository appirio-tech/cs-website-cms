<%= form_for @search do |f| %>
  <%= f.label :min_top_prize %>
  <%= f.number_field :min_top_prize %>

  <%= f.label :max_top_prize %>
  <%= f.number_field :max_top_prize %>

  <%= f.label :min_registered_members %>
  <%= f.number_field :min_registered_members %>

  <%= f.label :max_registered_members %>
  <%= f.number_field :max_registered_members %>
  <br />
  <%= f.submit %>
<% end %>